{
  "name": "flast",
  "description": "Cross-platform browser based on Chromium (Electron).",
  "version": "1.0.0-alpha.13",
  "license": "GPL-3.0-only",
  "author": {
    "name": "Aoichaan0513",
    "email": "aoichaan0513@gmail.com",
    "url": "https://aoichaan0513.jp"
  },
  "keywords": [
    "typescript",
    "chromium",
    "electron",
    "browser",
    "react",
    "styled-components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FascodeNet/flast_chromium.git"
  },
  "main": "build/main.js",
  "scripts": {
    "predev": "rimraf build",
    "dev": "run-p dev:*",
    "dev:electron": "wait-on ./build/app.html && cross-env NODE_ENV=\"development\" electron .",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "webpack --watch",
    "build": "webpack --config webpack.config.prod.ts --progress",
    "start": "electron .",
    "compile:win32": "npm run build && electron-builder -w",
    "compile:darwin": "npm run build && electron-builder -m",
    "compile:linux": "npm run build && electron-builder -l",
    "ci-compile:win32": "npm run build && electron-builder -w -p always",
    "ci-compile:darwin": "npm run build && electron-builder -m -p always",
    "ci-compile:linux": "npm run build && electron-builder -l -p always"
  },
  "dependencies": {
    "@emotion/react": "11.6.0",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.2.0",
    "@mui/lab": "5.0.0-alpha.57",
    "@mui/material": "5.2.1",
    "@mui/styles": "5.2.1",
    "animejs": "3.2.1",
    "clsx": "1.1.1",
    "color": "4.0.2",
    "deepmerge": "4.2.2",
    "electron-chrome-extensions": "3.9.0",
    "file-type": "17.0.2",
    "filesize": "^8.0.6",
    "format-number": "3.0.0",
    "icojs": "0.16.1",
    "image-data-uri": "2.0.1",
    "mobx": "6.3.8",
    "mobx-react-lite": "3.2.2",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "nanoid": "3.1.30",
    "nedb": "1.8.0",
    "node-fetch": "3.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-windows-controls": "1.1.1",
    "styled-components": "5.3.3",
    "tslint": "6.1.3"
  },
  "devDependencies": {
    "@electron/remote": "2.0.1",
    "@types/animejs": "3.1.4",
    "@types/chrome": "0.0.165",
    "@types/color": "3.0.2",
    "@types/filesize": "5.0.0",
    "@types/format-number": "3.0.0",
    "@types/mini-css-extract-plugin": "2.4.0",
    "@types/nedb": "1.8.12",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-helmet": "6.1.4",
    "@types/styled-components": "5.1.16",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "electron": "16.0.7",
    "electron-builder": "22.14.5",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.4.5",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.3",
    "wait-on": "6.0.0",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1"
  },
  "resolutions": {
    "follow-redirects": "1.14.7",
    "underscore": "1.13.1",
    "yargs-parser": "21.0.0"
  }
}
