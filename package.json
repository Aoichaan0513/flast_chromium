{
  "name": "flast-chromium",
  "description": "Cross-platform browser based on Chromium (Electron).",
  "version": "1.0.0-alpha.25",
  "license": "GPL-3.0-only",
  "author": {
    "name": "Aoichaan0513",
    "email": "aoichaan0513@gmail.com",
    "url": "https://aoichaan0513.jp"
  },
  "keywords": [
    "typescript",
    "chromium",
    "electron",
    "browser",
    "react",
    "styled-components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FascodeNet/flast_chromium.git"
  },
  "main": "build/main.js",
  "scripts": {
    "predev": "rimraf build",
    "dev": "run-p dev:*",
    "dev:electron": "wait-on ./build/browser/app.html && cross-env NODE_ENV=\"development\" electron .",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "webpack --watch",
    "build": "webpack --config webpack.config.prod.ts --progress",
    "start": "electron .",
    "compile:win32": "npm run build && electron-builder -w",
    "compile:darwin": "npm run build && electron-builder -m",
    "compile:linux": "npm run build && electron-builder -l",
    "ci-compile:win32": "npm run build && electron-builder -w -p always",
    "ci-compile:darwin": "npm run build && electron-builder -m -p always",
    "ci-compile:linux": "npm run build && electron-builder -l -p always"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.6.2",
    "@mui/lab": "5.0.0-alpha.79",
    "@mui/material": "5.6.3",
    "@mui/styles": "5.6.2",
    "animejs": "3.2.1",
    "clsx": "1.1.1",
    "color": "4.2.1",
    "deepmerge": "4.2.2",
    "electron-chrome-extensions": "3.9.0",
    "file-type": "17.1.1",
    "filesize": "8.0.7",
    "format-number": "3.0.0",
    "icojs": "0.17.0",
    "moment": "2.29.2",
    "moment-timezone": "0.5.34",
    "nanoid": "3.3.1",
    "nedb": "1.8.0",
    "node-fetch": "3.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-windows-controls": "1.1.1",
    "styled-components": "5.3.3",
    "tslint": "6.1.3"
  },
  "devDependencies": {
    "@electron/remote": "2.0.8",
    "@faker-js/faker": "6.0.0",
    "@types/animejs": "3.1.4",
    "@types/chrome": "0.0.180",
    "@types/color": "3.0.3",
    "@types/filesize": "5.0.0",
    "@types/format-number": "3.0.0",
    "@types/mini-css-extract-plugin": "2.5.1",
    "@types/nedb": "1.8.12",
    "@types/node": "17.0.21",
    "@types/react": "17.0.41",
    "@types/react-dom": "17.0.14",
    "@types/react-helmet": "6.1.5",
    "@types/styled-components": "5.1.24",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "electron": "18.2.0",
    "electron-builder": "23.0.3",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "typescript": "4.6.2",
    "wait-on": "6.0.1",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2"
  },
  "resolutions": {
    "follow-redirects": "1.14.8",
    "minimist": "1.2.6",
    "underscore": "1.13.1",
    "yargs-parser": "21.0.0"
  }
}
